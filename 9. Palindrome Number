class Solution {
    public boolean isPalindrome(int x) {
        
        
        boolean isPalindrome = false;
        String s = Integer.toString(x);
        int len = s.length()-1;
        
        if (len ==0 ) {
            return true;
        }
        
        for (int i=0; i<len; i++) {
            
            if (s.charAt(i) == s.charAt(len-i)) {
                isPalindrome = true;
            } else {
                return false;
            }
        
        }
    return isPalindrome;
    }
}

// Time Complexity: O(n)
